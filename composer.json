{
  "name": "hs/webphp",
  "type": "project",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.4",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "doctrine/dbal": "^4.3.4",
    "doctrine/doctrine-bundle": "^3.0.0",
    "doctrine/doctrine-migrations-bundle": "^3.5",
    "doctrine/orm": "^3.5.2",
    "dompdf/dompdf": "^3.1",
    "symfony/asset": "7.3.*",
    "symfony/asset-mapper": "7.3.*",
    "symfony/browser-kit": "7.3.*",
    "symfony/clock": "7.3.*",
    "symfony/console": "7.3.*",
    "symfony/css-selector": "7.3.*",
    "symfony/doctrine-messenger": "7.3.*",
    "symfony/dotenv": "7.3.*",
    "symfony/flex": "^2.8.2",
    "symfony/form": "7.3.*",
    "symfony/framework-bundle": "7.3.*",
    "symfony/mailer": "7.3.*",
    "symfony/messenger": "7.3.*",
    "symfony/monolog-bundle": "^3.10",
    "symfony/password-hasher": "7.3.*",
    "symfony/runtime": "7.3.*",
    "symfony/security-bundle": "7.3.*",
    "symfony/stimulus-bundle": "^2.31",
    "symfony/string": "7.3.*",
    "symfony/translation": "7.3.*",
    "symfony/twig-bundle": "7.3.*",
    "symfony/ux-live-component": "^2.31",
    "symfony/ux-twig-component": "^2.31",
    "symfony/validator": "7.3.*",
    "symfony/yaml": "7.3.*",
    "symfonycasts/reset-password-bundle": "^1.23",
    "symfonycasts/tailwind-bundle": "^0.11.1",
    "twig/extra-bundle": "^3.21",
    "twig/intl-extra": "^3.21",
    "twig/string-extra": "^3.21",
    "twig/twig": "^2.12|^3.21.1"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "phpstan/extension-installer": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "bump-after-update": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php82": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd",
      "importmap:install": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "7.3.*"
    }
  },
  "require-dev": {
    "dama/doctrine-test-bundle": "^8.4",
    "doctrine/doctrine-fixtures-bundle": "^4.3",
    "friendsofphp/php-cs-fixer": "^3.89.1",
    "phpstan/extension-installer": "^1.4",
    "phpstan/phpstan": "^2.1",
    "phpstan/phpstan-deprecation-rules": "^2.0",
    "phpstan/phpstan-doctrine": "^2.0",
    "phpstan/phpstan-phpunit": "^2.0",
    "phpstan/phpstan-strict-rules": "^2.0",
    "phpstan/phpstan-symfony": "^2.0",
    "phpunit/phpunit": "^12.4.1",
    "spaze/phpstan-disallowed-calls": "^4.6",
    "staabm/phpstan-psr3": "^1.0.3",
    "symfony/maker-bundle": "^1.64"
  }
}
